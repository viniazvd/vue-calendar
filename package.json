    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "autoprefixer": "^9.4.2",
    "babel-core": "^7.0.0-beta",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^7.0.0-beta",
    "bili": "^3.4.2",
